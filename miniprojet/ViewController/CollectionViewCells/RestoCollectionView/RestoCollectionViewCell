import UIKit
import Kingfisher

class RestoCollectionViewCell: UICollectionViewCell {
    static let identifier = "RestoCellView"
    
    @IBOutlet weak var titleLabel: UILabel!
    
    
    @IBOutlet weak var RestoImage: UIImageView!
    
    @IBOutlet weak var categorieLbl: UILabel!
    
    func setup (resto: DishResto) {
        titleLabel.text = resto.name
        RestoImage.kf.setImage(with: resto.picture.asUrl)
        categorieLbl.text = resto.name
        
    }
}
